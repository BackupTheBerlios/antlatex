@gd{Zustandsdiagramm,
    word={Zustandsdiagramm},
  definition={
Grafische Darstellungsform eines Zustandsautomaten. Die Zustände
werden in der UML als Rechtecke mit abgerundeten Ecken dargestellt. Die Zustandsübergänge werden durch beschriftete Pfeile
angegeben (Eingabe/Ausgabe).

}
}
@gd{HTML,
    word={HyperText Markup Language},
  short={HTML},
  definition={
Seitenauszeichnungssprache, die es mit Hilfe von Formatbefehlen (tags) erlaubt,
inhaltliche Kategorien von WWW-Dokumenten, z.B. Überschriften, Absätze, sowie
Hyperlinks zu kennzeichnen. So ausgezeichnete Dokumente
werden von WWW-Browsern interpretiert und
dargestellt. Dateiendung .html bzw. htm.

}
}
